{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "run_fastsurfer.sh",
  "description": "run_fastsurfer.sh takes a T1 full head image and creates segmentation using FastSurferVINN and surfaces using recon-surf",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "segmentation": {
      "type": "string",
      "title": "Segmentation Output",
      "description": "FastSurferVINN segmentation output"
    },
    "surface_files": {
      "type": "string",
      "title": "Surface Files",
      "description": "Generated surface files"
    },
    "cerebellum_seg": {
      "type": "string",
      "title": "Cerebellum Segmentation",
      "description": "Cerebellum segmentation output"
    },
    "qc_snapshots": {
      "type": "string",
      "title": "QC Snapshots",
      "description": "Quality control snapshot images"
    },
    "bias_corrected": {
      "type": "string",
      "title": "Bias Corrected Image",
      "description": "Bias field corrected image"
    }
  }
}