{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "A tool for high-resolution registration for Freesurfer images",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "mri_hires_register"
    },
    "hires_labeling": {
      "type": "string",
      "title": "High Resolution Labeling",
      "description": "The high resolution labeling input file"
    },
    "input_intensity": {
      "type": "string",
      "title": "Input Intensity",
      "description": "The input intensity file"
    },
    "input_aseg": {
      "type": "string",
      "title": "Input Aseg",
      "description": "The input aseg file"
    },
    "output_xform": {
      "type": "string",
      "title": "Output Transform",
      "description": "The output transform file"
    }
  },
  "additionalProperties": false,
  "required": [
    "hires_labeling",
    "input_intensity",
    "input_aseg",
    "output_xform"
  ]
}