{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Prints the list of unique labels (with structure name) in the input volume.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.print_unique_labels.csh"
    },
    "label_volume": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Label Volume",
      "description": "Label volume to be analyzed"
    },
    "output_file": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output File",
      "description": "Text file where the results are written"
    },
    "list_only": {
      "type": "boolean",
      "default": false,
      "title": "List Only",
      "description": "Only list the labels"
    },
    "version": {
      "type": "boolean",
      "default": false,
      "title": "Print Version",
      "description": "Print version and exit"
    },
    "help": {
      "type": "boolean",
      "default": false,
      "title": "Print Help",
      "description": "Print help and exit"
    }
  },
  "additionalProperties": false,
  "required": [
    "label_volume"
  ]
}