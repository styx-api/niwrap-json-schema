{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "mris_interpolate_warp",
  "description": "Interpolate warp on cortical surfaces",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer/mris_interpolate_warp"
    },
    "start_surface": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Start Surface",
      "description": "The starting surface file."
    },
    "end_surface": {
      "type": "string",
      "x-styx-type": "file",
      "title": "End Surface",
      "description": "The ending surface file."
    },
    "warp_field": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Warp Field",
      "description": "The warp field file with .m3z extension."
    }
  },
  "additionalProperties": false,
  "required": [
    "start_surface",
    "end_surface",
    "warp_field",
    "@type"
  ]
}