{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Command-line tool for printing CSV files",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.csvprint"
    },
    "infile": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Input CSV file",
      "description": "Input CSV file to be printed"
    }
  },
  "additionalProperties": false,
  "required": [
    "infile"
  ]
}