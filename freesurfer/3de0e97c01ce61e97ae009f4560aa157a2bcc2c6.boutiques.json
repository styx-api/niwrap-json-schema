{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Script to plot structural statistics",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.plot_structure_stats.tcl"
    },
    "input_file": {
      "type": "string",
      "title": "Input File",
      "description": "Input stats file"
    },
    "output_file": {
      "type": "string",
      "title": "Output File",
      "description": "Output plot file"
    }
  },
  "additionalProperties": false,
  "required": [
    "input_file",
    "output_file"
  ]
}