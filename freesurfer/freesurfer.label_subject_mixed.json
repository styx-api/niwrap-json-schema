{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Automatic labeling of brain regions using a Gaussian classifier array.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.label_subject_mixed"
    },
    "brain_mask": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Brain Mask",
      "description": "Brain mask to be used for final labeling"
    },
    "norm_volume": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Normalized Volume",
      "description": "Normalized volume"
    },
    "transform": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Transform File",
      "description": "Transform file in LTA format"
    },
    "gca_file": {
      "type": "string",
      "x-styx-type": "file",
      "title": "GCA File",
      "description": "Gaussian classifier array (GCA) file"
    },
    "aseg_output": {
      "type": "string",
      "title": "Aseg Output",
      "description": "Output aseg file"
    }
  },
  "additionalProperties": false,
  "required": [
    "brain_mask",
    "norm_volume",
    "transform",
    "gca_file",
    "aseg_output"
  ]
}