{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "rbbr",
  "description": "Robust version of bbregister",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "output_reg_file": {
      "type": "string",
      "title": "Output Registration File",
      "description": "Output registration file"
    },
    "output_lta_file": {
      "type": "string",
      "title": "Output LTA File",
      "description": "Output LTA file"
    },
    "output_template_file": {
      "type": "string",
      "title": "Output Template File",
      "description": "Saved template output file"
    }
  }
}