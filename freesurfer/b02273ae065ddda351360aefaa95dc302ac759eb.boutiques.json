{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Tridec tool for processing brain images.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.tridec"
    },
    "subject_name": {
      "type": "string",
      "title": "Subject Name",
      "description": "Name of the subject being processed."
    },
    "fine_file": {
      "type": "string",
      "title": "Fine File",
      "description": "Fine file input for tridec."
    },
    "ico_file": {
      "type": "string",
      "title": "ICO File",
      "description": "ICO file input for tridec."
    },
    "out_file": {
      "type": "string",
      "title": "Output File",
      "description": "Output file for tridec processing result."
    }
  },
  "additionalProperties": false,
  "required": [
    "subject_name",
    "fine_file",
    "ico_file",
    "out_file"
  ]
}