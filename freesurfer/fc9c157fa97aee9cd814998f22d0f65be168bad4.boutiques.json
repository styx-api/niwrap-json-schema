{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Performs a logical voxel-wise AND on a series of volumes",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "mri_and"
    },
    "input_files": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "title": "Input files",
      "description": "Input volume files"
    }
  },
  "additionalProperties": false,
  "required": [
    "input_files"
  ]
}