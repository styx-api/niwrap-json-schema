{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Tool to unpack image directories, from FreeSurfer suite.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.unpackimadir2"
    },
    "directory": {
      "type": "string",
      "title": "Input directory",
      "description": "Image directory to unpack."
    }
  },
  "additionalProperties": false,
  "required": [
    "directory"
  ]
}