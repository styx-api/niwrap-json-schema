{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "mri_gtmpvc",
  "description": "mri_gtmpvc performs partial volume correction on PET data using anatomical segmentation.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "synthesized_volume": {
      "type": "string",
      "title": "Synthesized Volume",
      "description": "Synthesized volume"
    },
    "rescaled_input": {
      "type": "string",
      "title": "Rescaled Input",
      "description": "Rescaled input volume"
    },
    "eresiduals": {
      "type": "string",
      "title": "Residual Errors",
      "description": "Residual errors volume"
    },
    "yhat": {
      "type": "string",
      "title": "Yhat",
      "description": "Yhat volume"
    },
    "yhat_with_noise": {
      "type": "string",
      "title": "Yhat with Noise",
      "description": "Yhat with noise volume"
    },
    "yhat_full_fov": {
      "type": "string",
      "title": "Yhat Full FoV",
      "description": "Yhat full FoV volume"
    },
    "yhat_prior": {
      "type": "string",
      "title": "Yhat Prior",
      "description": "Yhat prior to smoothing volume"
    },
    "x_matrix": {
      "type": "string",
      "title": "X Matrix",
      "description": "X matrix in matlab4 format"
    },
    "y_matrix": {
      "type": "string",
      "title": "Y Matrix",
      "description": "Y matrix in matlab4 format"
    },
    "beta_matrix": {
      "type": "string",
      "title": "Beta Matrix",
      "description": "Beta matrix in matlab4 format"
    },
    "x0_matrix": {
      "type": "string",
      "title": "X0 Matrix",
      "description": "X0 matrix in matlab4 format"
    },
    "gtm_values_text": {
      "type": "string",
      "title": "GTM Values Text",
      "description": "Demeaned GTM values as text file"
    }
  }
}