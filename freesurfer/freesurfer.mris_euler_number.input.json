{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "mris_euler_number",
  "description": "This program computes EulerNumber for a cortical surface",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer/mris_euler_number"
    },
    "input_surface": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Input Surface",
      "description": "Input surface file"
    },
    "output_file": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output File",
      "description": "Write number of holes to output file"
    }
  },
  "additionalProperties": false,
  "required": [
    "input_surface",
    "@type"
  ]
}