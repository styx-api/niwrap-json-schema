{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "register_elderly_subject",
  "description": "Tool for registering MRI images of elderly subjects using Freesurfer's mri_em_register",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "transformed_fsamples_output": {
      "type": "string",
      "title": "Transformed Fs Samples",
      "description": "Transformed control points output"
    },
    "normalized_volume_output": {
      "type": "string",
      "title": "Normalized Volume",
      "description": "Intensity normalized volume"
    }
  }
}