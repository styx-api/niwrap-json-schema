{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Tool for atlas renormalization in FreeSurfer",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "mri_cal_renormalize_gca"
    },
    "timepoint_file": {
      "type": "string",
      "title": "Timepoint file",
      "description": "Longitudinal time point file"
    },
    "in_vol": {
      "type": "string",
      "title": "Input volume",
      "description": "Input volume"
    },
    "input_atlas": {
      "type": "string",
      "title": "Input atlas",
      "description": "Input atlas"
    },
    "transform_file": {
      "type": "string",
      "title": "Transform file",
      "description": "Transform file"
    },
    "output_atlas": {
      "type": "string",
      "title": "Output atlas",
      "description": "Output atlas"
    }
  },
  "additionalProperties": false,
  "required": [
    "timepoint_file",
    "in_vol",
    "input_atlas",
    "transform_file",
    "output_atlas"
  ]
}