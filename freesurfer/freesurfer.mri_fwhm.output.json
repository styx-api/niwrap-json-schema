{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "mri_fwhm",
  "description": "FreeSurfer program to estimate the global Gaussian smoothness of a multi-frame, volume-based data set.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "output_volume_file": {
      "type": "string",
      "title": "Output Volume File",
      "description": "Output volume after smoothing."
    },
    "final_mask_output_file": {
      "type": "string",
      "title": "Final Mask Output File",
      "description": "Final mask volume."
    },
    "summary_log_file": {
      "type": "string",
      "title": "Summary Log File",
      "description": "Summary log file."
    },
    "final_fwhm_estimate_file": {
      "type": "string",
      "title": "Final FWHM Estimate File",
      "description": "Final FWHM estimate file."
    },
    "fwhm_of_each_dimension_file": {
      "type": "string",
      "title": "FWHM of Each Dimension File",
      "description": "File containing the FWHM of each dimension."
    },
    "mean_fwhm_volume_file": {
      "type": "string",
      "title": "Mean FWHM Volume File",
      "description": "Mean FWHM from volume file."
    },
    "fwhm_volume_file": {
      "type": "string",
      "title": "FWHM Volume File",
      "description": "FWHM volume file."
    }
  }
}