{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "A tool for cubic spline interpolation.",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "Spline3_test"
    },
    "x_values": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "title": "X values",
      "description": "Original x values for interpolation."
    },
    "y_values": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "title": "Y values",
      "description": "Original y values corresponding to x values."
    },
    "x_new_values": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "title": "X new values",
      "description": "New x values where interpolation is evaluated."
    }
  },
  "additionalProperties": false,
  "required": [
    "x_values",
    "y_values",
    "x_new_values"
  ]
}