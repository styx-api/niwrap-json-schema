{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "No description",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.mri_head"
    },
    "identify": {
      "type": "boolean",
      "default": false,
      "title": "Identify Flag",
      "description": "Identify the MRI file"
    },
    "read": {
      "type": "boolean",
      "default": false,
      "title": "Read Flag",
      "description": "Read the MRI file"
    },
    "filename": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Filename",
      "description": "Filename for identification or reading"
    },
    "help": {
      "type": "boolean",
      "default": false,
      "title": "Help Flag",
      "description": "Display help information"
    },
    "usage": {
      "type": "boolean",
      "default": false,
      "title": "Usage Flag",
      "description": "Display usage information"
    },
    "question_mark_help": {
      "type": "boolean",
      "default": false,
      "title": "Question Mark Help Flag",
      "description": "Display help or usage information"
    }
  },
  "additionalProperties": false
}