{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fs_spmreg.glnxa64",
  "description": "fs_spmreg is a tool for registration using SPM within FreeSurfer.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.fs_spmreg.glnxa64"
    },
    "input_volume": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Input Volume",
      "description": "Input anatomical volume"
    },
    "output_matrix": {
      "type": "string",
      "default": "output.mat",
      "title": "Output Matrix",
      "description": "Output registration matrix"
    }
  },
  "additionalProperties": false,
  "required": [
    "input_volume"
  ]
}