{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "compute_vox2vox",
  "description": "Tool for computing voxel-to-voxel transformations.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.compute_vox2vox"
    },
    "source": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Source File",
      "description": "Input 4dfp source file."
    },
    "t4file": {
      "type": "string",
      "x-styx-type": "file",
      "title": "T4 File",
      "description": "T4 transformation matrix file."
    },
    "target": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Target File",
      "description": "Input 4dfp target file."
    }
  },
  "additionalProperties": false,
  "required": [
    "source",
    "t4file",
    "target"
  ]
}