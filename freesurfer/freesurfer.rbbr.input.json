{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "rbbr",
  "description": "Robust version of bbregister",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer.rbbr"
    },
    "subject": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Subject",
      "description": "FreeSurfer subject (not needed with --init-reg)"
    },
    "moving_image": {
      "type": "string",
      "title": "Moving Image",
      "description": "Input moving image"
    },
    "t1_contrast": {
      "type": "boolean",
      "default": false,
      "title": "T1 Contrast",
      "description": "Use T1 tissue contrast"
    },
    "t2_contrast": {
      "type": "boolean",
      "default": false,
      "title": "T2 Contrast",
      "description": "Use T2 tissue contrast"
    },
    "init_reg": {
      "type": "boolean",
      "default": false,
      "title": "Init Reg",
      "description": "Initialize registration"
    },
    "init_spm": {
      "type": "boolean",
      "default": false,
      "title": "Init SPM",
      "description": "Initialize with SPM"
    },
    "init_fsl": {
      "type": "boolean",
      "default": false,
      "title": "Init FSL",
      "description": "Initialize with FSL"
    },
    "init_header": {
      "type": "boolean",
      "default": false,
      "title": "Init Header",
      "description": "Initialize using header"
    },
    "cost_threshold": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Cost Threshold",
      "description": "Cost threshold to define outlier"
    },
    "gtm_synthesize": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "GTM Synthesize",
      "description": "Use GTM to synthesize"
    },
    "tt_reduce": {
      "type": "boolean",
      "default": false,
      "title": "TT Reduce Flag",
      "description": "Reduce GTM Seg to tissue types for faster processing"
    },
    "iterations": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Iterations",
      "description": "Number of iterations"
    },
    "output_reg": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output Reg",
      "description": "Output registration file"
    },
    "output_lta": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output LTA",
      "description": "Output LTA file"
    },
    "left_hemi": {
      "type": "boolean",
      "default": false,
      "title": "Left Hemisphere Only",
      "description": "Only use left hemisphere"
    },
    "right_hemi": {
      "type": "boolean",
      "default": false,
      "title": "Right Hemisphere Only",
      "description": "Only use right hemisphere"
    },
    "gm_proj_frac": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "GM Projection Fraction",
      "description": "GM projection fraction"
    },
    "gm_proj_abs": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "GM Projection Absolute",
      "description": "GM projection absolute"
    },
    "wm_proj_abs": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "WM Projection Absolute",
      "description": "WM projection absolute"
    },
    "frame_no": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Frame Number",
      "description": "Use 0-based frame number as template"
    },
    "output_template": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output Template",
      "description": "Save template as an output"
    },
    "no_merge": {
      "type": "boolean",
      "default": false,
      "title": "No Merge Flag",
      "description": "Do not merge GTM Ids"
    }
  },
  "additionalProperties": false,
  "required": [
    "moving_image"
  ]
}