{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fsl_label2voxel",
  "description": "Converts labeled volumes to voxels based on specified labels",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer/fsl_label2voxel"
    },
    "label_value": {
      "type": "number",
      "title": "Label value",
      "description": "Label value to convert"
    },
    "labeled_volume": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Labeled volume",
      "description": "Labeled volume file"
    },
    "src_volume": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Source volume",
      "description": "Source volume file"
    },
    "output_filename": {
      "type": "string",
      "title": "Output filename",
      "description": "Output filename for voxel data"
    }
  },
  "additionalProperties": false,
  "required": [
    "label_value",
    "labeled_volume",
    "src_volume",
    "output_filename"
  ]
}