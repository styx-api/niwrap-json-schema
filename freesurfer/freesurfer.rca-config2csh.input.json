{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "rca-config2csh",
  "description": "rca-config2csh is a utility to convert configuration files into C-shell syntax.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "freesurfer/rca-config2csh"
    },
    "configfile": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Configuration file",
      "description": "Configuration file to be converted"
    }
  },
  "additionalProperties": false,
  "required": [
    "configfile",
    "@type"
  ]
}