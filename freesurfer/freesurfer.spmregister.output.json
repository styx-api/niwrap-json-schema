{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "spmregister",
  "description": "Registers a volume to its FreeSurfer anatomical using SPM's spm_coreg",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "registration_file": {
      "type": "string",
      "title": "Registration file",
      "description": "Output registration file."
    },
    "resampled_mov": {
      "type": "string",
      "title": "Resampled movable volume",
      "description": "Resampled mov volume."
    }
  }
}