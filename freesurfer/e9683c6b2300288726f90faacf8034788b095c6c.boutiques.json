{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Corrects subject data in FreeSurfer",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "fix_subject_corrected"
    },
    "subject_directory": {
      "type": "string",
      "title": "Subject Directory",
      "description": "Path to the subject's directory"
    },
    "output_directory": {
      "type": "string",
      "title": "Output Directory",
      "description": "Path to the output directory"
    }
  },
  "additionalProperties": false,
  "required": [
    "subject_directory",
    "output_directory"
  ]
}