{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "file-convert",
  "description": "Change version of file format.\n\nYou may only specify one top-level option.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "workbench.file-convert"
    },
    "border_version_convert": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "workbench.file-convert.border_version_convert"
            },
            "border_in": {
              "type": "string",
              "x-styx-type": "file",
              "title": "border_in",
              "description": "the input border file"
            },
            "out_version": {
              "type": "integer",
              "title": "out_version",
              "description": "the format version to write as, 1 or 3 (2 doesn't exist)"
            },
            "border_out": {
              "type": "string",
              "title": "border_out",
              "description": "output - the output border file"
            },
            "opt_surface_surface": {
              "anyOf": [
                {
                  "type": "string",
                  "x-styx-type": "file"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "opt_surface_surface",
              "description": "must be specified if the input is version 1: use this surface file for structure and number of vertices, ignore borders on other structures"
            }
          },
          "additionalProperties": false,
          "title": "border_version_convert",
          "description": "write a border file with a different version",
          "required": [
            "border_in",
            "out_version",
            "border_out"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "border_version_convert",
      "description": "write a border file with a different version"
    },
    "nifti_version_convert": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "workbench.file-convert.nifti_version_convert"
            },
            "input": {
              "type": "string",
              "title": "input",
              "description": "the input nifti file"
            },
            "version": {
              "type": "integer",
              "title": "version",
              "description": "the nifti version to write as"
            },
            "output": {
              "type": "string",
              "title": "output",
              "description": "output - the output nifti file"
            }
          },
          "additionalProperties": false,
          "title": "nifti_version_convert",
          "description": "write a nifti file with a different version",
          "required": [
            "input",
            "version",
            "output"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "nifti_version_convert",
      "description": "write a nifti file with a different version"
    },
    "cifti_version_convert": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "workbench.file-convert.cifti_version_convert"
            },
            "cifti_in": {
              "type": "string",
              "x-styx-type": "file",
              "title": "cifti_in",
              "description": "the input cifti file"
            },
            "version": {
              "type": "string",
              "title": "version",
              "description": "the cifti version to write as"
            },
            "cifti_out": {
              "type": "string",
              "title": "cifti_out",
              "description": "output - the output cifti file"
            }
          },
          "additionalProperties": false,
          "title": "cifti_version_convert",
          "description": "write a cifti file with a different version",
          "required": [
            "cifti_in",
            "version",
            "cifti_out"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "cifti_version_convert",
      "description": "write a cifti file with a different version"
    }
  },
  "additionalProperties": false
}