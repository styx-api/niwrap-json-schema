{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "TRANSPOSE A CIFTI FILE.\n\nThe input must be a 2-dimensional cifti file.  The output is a cifti file where every row in the input is a column in the output.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "workbench/cifti-transpose"
    },
    "cifti-out": {
      "type": "string",
      "description": "the output cifti file"
    },
    "limit-GB": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "restrict memory usage\n\nmemory limit in gigabytes"
    },
    "cifti-in": {
      "type": "string",
      "x-styx-type": "file",
      "description": "the input cifti file"
    }
  },
  "additionalProperties": false,
  "required": [
    "cifti-out",
    "cifti-in",
    "@type"
  ]
}