{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "parse_fs_lt_log.py",
  "description": "Parses FreeSurfer labeltable log file and retrieves labeltable indices.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/parse_fs_lt_log.py"
    },
    "logfile": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Log file",
      "description": "Specify rank log file."
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "title": "Labels",
      "description": "Specify a list of labels to search for."
    },
    "show_orig": {
      "type": "boolean",
      "default": false,
      "title": "Show Original",
      "description": "Show original label indices."
    },
    "show_all_orig": {
      "type": "boolean",
      "default": false,
      "title": "Show All Original",
      "description": "Show all original label indices."
    },
    "verbosity": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Verbosity",
      "description": "Specify verbosity level."
    }
  },
  "additionalProperties": false,
  "required": [
    "logfile",
    "labels",
    "@type"
  ]
}