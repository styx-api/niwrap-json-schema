{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SurfDsetInfo",
  "description": "Provides information about surface datasets (DSET).",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.SurfDsetInfo"
    },
    "input_dsets": {
      "type": "array",
      "items": {
        "type": "string",
        "x-styx-type": "file"
      },
      "title": "Input Datasets",
      "description": "Input dataset"
    },
    "debug_level": {
      "anyOf": [
        {
          "type": "integer",
          "minimum": 0
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Debug Level",
      "description": "Debug level. If DBG = 2, show full dataset information in NIML form."
    },
    "novolreg": {
      "type": "boolean",
      "default": false,
      "title": "Ignore Volume Registration Transformations",
      "description": "Ignore any Rotate, Volreg, Tagalign, or WarpDrive transformations"
    },
    "noxform": {
      "type": "boolean",
      "default": false,
      "title": "No Transformations",
      "description": "Same as -novolreg"
    },
    "setenv": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Set Environment Variable",
      "description": "Set environment variable"
    },
    "trace": {
      "type": "boolean",
      "default": false,
      "title": "IO and Memory Tracing",
      "description": "Turns on In/Out debug and Memory tracing"
    },
    "extreme_trace": {
      "type": "boolean",
      "default": false,
      "title": "Extreme Tracing",
      "description": "Turns on extreme tracing"
    },
    "nomall": {
      "type": "boolean",
      "default": false,
      "title": "Turn Off Memory Tracing",
      "description": "Turn off memory tracing"
    },
    "yesmall": {
      "type": "boolean",
      "default": false,
      "title": "Turn On Memory Tracing",
      "description": "Turn on memory tracing (default)"
    },
    "mini_help": {
      "type": "boolean",
      "default": false,
      "title": "Mini Help",
      "description": "Mini help"
    },
    "help": {
      "type": "boolean",
      "default": false,
      "title": "Help",
      "description": "Show entire help output"
    },
    "extreme_help": {
      "type": "boolean",
      "default": false,
      "title": "Extreme Help",
      "description": "Show extreme help"
    },
    "help_view": {
      "type": "boolean",
      "default": false,
      "title": "Help View",
      "description": "Open help in text editor"
    },
    "help_web": {
      "type": "boolean",
      "default": false,
      "title": "Help Web",
      "description": "Open help in web browser"
    },
    "help_find": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Help Find Word",
      "description": "Look for lines in help output that match the specified word"
    },
    "help_raw": {
      "type": "boolean",
      "default": false,
      "title": "Help Raw",
      "description": "Show unedited help string"
    },
    "help_spx": {
      "type": "boolean",
      "default": false,
      "title": "Help SPX",
      "description": "Show help string in sphinx format, but do not autoformat"
    },
    "help_aspx": {
      "type": "boolean",
      "default": false,
      "title": "Help ASPX",
      "description": "Show help string in sphinx with autoformatting"
    },
    "all_opts": {
      "type": "boolean",
      "default": false,
      "title": "All Options",
      "description": "Attempt to identify all options for the program from the help output"
    }
  },
  "additionalProperties": false,
  "required": [
    "input_dsets"
  ]
}