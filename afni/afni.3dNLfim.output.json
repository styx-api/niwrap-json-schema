{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "3dNLfim",
  "description": "Nonlinear regression for each voxel of the input AFNI 3d+time data set",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "freg_outfile": {
      "type": "string",
      "title": "F-test output file",
      "description": "F-test for significance of the regression"
    },
    "frsqr_outfile": {
      "type": "string",
      "title": "R^2 output file",
      "description": "R^2 calculation for regression"
    },
    "fsmax_outfile": {
      "type": "string",
      "title": "Signed maximum signal output file",
      "description": "Signed maximum signal estimate"
    },
    "ftmax_outfile": {
      "type": "string",
      "title": "Time of signed maximum output file",
      "description": "Time of signed maximum estimate"
    },
    "fpsmax_outfile": {
      "type": "string",
      "title": "Maximum percentage change output file",
      "description": "Maximum percentage change estimate"
    },
    "farea_outfile": {
      "type": "string",
      "title": "Area between signal and baseline output file",
      "description": "Area between signal and baseline"
    },
    "fparea_outfile": {
      "type": "string",
      "title": "Percentage area output file",
      "description": "Percentage area of signal estimate"
    },
    "fscoef_outfile": {
      "type": "string",
      "title": "Signal parameter estimate output file",
      "description": "Signal parameter estimate"
    },
    "fncoef_outfile": {
      "type": "string",
      "title": "Noise parameter estimate output file",
      "description": "Noise parameter estimate"
    },
    "tscoef_outfile": {
      "type": "string",
      "title": "Signal parameter T-test output file",
      "description": "T-test for significance of signal parameter"
    },
    "tncoef_outfile": {
      "type": "string",
      "title": "Noise parameter T-test output file",
      "description": "T-test for significance of noise parameter"
    },
    "bucket_outfile": {
      "type": "string",
      "title": "AFNI bucket",
      "description": "AFNI 'bucket' dataset"
    },
    "sfit_outfile": {
      "type": "string",
      "title": "Signal fit output file",
      "description": "Output 3d+time signal model fit"
    },
    "snfit_outfile": {
      "type": "string",
      "title": "Signal+noise fit output file",
      "description": "Output 3d+time signal+noise fit"
    }
  }
}