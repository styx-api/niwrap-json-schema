{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "3dmaskave",
  "description": "Computes average of all voxels in the input dataset which satisfy the criterion in the options list",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.3dmaskave"
    },
    "in_file": {
      "type": "string",
      "x-styx-type": "file",
      "title": "In file",
      "description": "Input file to 3dmaskave."
    },
    "mask": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Mask",
      "description": "Matrix to align input file."
    },
    "num_threads": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Num threads",
      "description": "Set number of threads."
    },
    "outputtype": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Outputtype",
      "description": "'nifti' or 'afni' or 'nifti_gz'. Afni output filetype."
    },
    "quiet": {
      "type": "boolean",
      "default": false,
      "title": "Quiet",
      "description": "Matrix to align input file."
    }
  },
  "additionalProperties": false,
  "required": [
    "in_file"
  ]
}