{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "get_afni_model_PRF",
  "description": "A tool to get AFNI model parameters assuming a PRF framework",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/get_afni_model_PRF"
    },
    "amplitude": {
      "type": "number",
      "title": "Amplitude",
      "description": "Amplitude for the AFNI model"
    },
    "x_coord": {
      "type": "number",
      "title": "X-coordinate",
      "description": "X-coordinate for the AFNI model"
    },
    "y_coord": {
      "type": "number",
      "title": "Y-coordinate",
      "description": "Y-coordinate for the AFNI model"
    },
    "sigma": {
      "type": "number",
      "title": "Sigma",
      "description": "Sigma value for the AFNI model"
    }
  },
  "additionalProperties": false,
  "required": [
    "amplitude",
    "x_coord",
    "y_coord",
    "sigma",
    "@type"
  ]
}