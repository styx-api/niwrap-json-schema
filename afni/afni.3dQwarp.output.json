{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "3dQwarp",
  "description": "Computes a nonlinearly warped version of source_dataset to match base_dataset.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "warped_dataset": {
      "type": "string",
      "title": "Warped Dataset",
      "description": "Warped dataset"
    },
    "warp_dataset": {
      "type": "string",
      "title": "Warp Dataset",
      "description": "Warp dataset"
    },
    "inverse_warp_dataset": {
      "type": "string",
      "title": "Inverse Warp Dataset",
      "description": "Inverse warp dataset"
    }
  }
}