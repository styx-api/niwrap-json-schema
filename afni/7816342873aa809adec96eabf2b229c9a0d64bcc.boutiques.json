{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Swaps byte pairs on the files listed",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "2swap"
    },
    "quiet": {
      "type": "boolean",
      "default": false,
      "title": "Quiet flag",
      "description": "Work quietly"
    },
    "input_files": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "title": "Input files",
      "description": "Input files"
    }
  },
  "additionalProperties": false,
  "required": [
    "input_files"
  ]
}