{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "@djunct_4d_imager",
  "description": "The program is useful for viewing the same slice across the 'time' dimension of a 4D data set.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/@djunct_4d_imager"
    },
    "inset": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Input Dataset",
      "description": "ULay dataset, probably 4D (required)"
    },
    "prefix": {
      "type": "string",
      "title": "Output Prefix",
      "description": "Prefix for output files (required)"
    },
    "do_movie": {
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "MPEG",
            "AGIF"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Movie Type",
      "description": "Specify type of movie file. Options: MPEG, AGIF"
    },
    "no_clean": {
      "type": "boolean",
      "default": false,
      "title": "No Clean Flag",
      "description": "Keep the final intermediate files"
    }
  },
  "additionalProperties": false,
  "required": [
    "inset",
    "prefix",
    "@type"
  ]
}