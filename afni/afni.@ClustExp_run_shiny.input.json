{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "@ClustExp_run_shiny",
  "description": "Launch a shiny app that was created by ClustExp_StatParse.py",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/@ClustExp_run_shiny"
    },
    "directory": {
      "type": "string",
      "title": "Input Directory",
      "description": "Folder created by ClustExp_StatParse.py"
    },
    "help": {
      "type": "boolean",
      "default": false,
      "title": "Help Flag",
      "description": "Show help message"
    }
  },
  "additionalProperties": false,
  "required": [
    "directory",
    "@type"
  ]
}