{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "An adjunct program for making TSNR plots for APQC.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.adjunct_apqc_tsnr_general"
    },
    "montgap": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Montage gap",
      "description": "Specify montage gap."
    },
    "montcolor": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Montage color",
      "description": "Specify montage color."
    },
    "montx": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Montage x coordinate",
      "description": "Specify montage x coordinate."
    },
    "monty": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Montage y coordinate",
      "description": "Specify montage y coordinate."
    },
    "opacity": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Overlay opacity",
      "description": "Specify overlay opacity."
    },
    "blowup": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Blowup factor",
      "description": "Specify blowup factor."
    },
    "save_ftype": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Save file type",
      "description": "Specify save file type."
    },
    "set_dicom_xyz": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 3
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "DICOM coordinates",
      "description": "Set DICOM x, y, z coordinates."
    },
    "set_ijk": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 3
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "IJK coordinates",
      "description": "Set IJK coordinates."
    },
    "set_subbricks": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 3
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Sub-bricks",
      "description": "Set sub-bricks."
    },
    "olay_alpha": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Overlay alpha",
      "description": "Specify overlay alpha."
    },
    "olay_boxed": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Boxed overlay",
      "description": "Specify boxed overlay."
    },
    "thr_olay": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Overlay threshold",
      "description": "Specify threshold for overlay."
    },
    "ulay_range_nz": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 2
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Underlay range non-zero",
      "description": "Specify non-zero range for underlay."
    },
    "ulay_range": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 2
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Underlay range",
      "description": "Specify range for underlay."
    },
    "delta_slices": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 3
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Delta slices",
      "description": "Specify delta slices."
    },
    "olay_disc_hot_range": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 2
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Overlay hot range",
      "description": "Specify discrete hot range for overlay."
    },
    "olay_cont_max": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Overlay continuous max",
      "description": "Specify continuous max for overlay."
    },
    "cbar_cont": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Continuous color bar",
      "description": "Specify continuous color bar."
    },
    "no_cor": {
      "type": "boolean",
      "default": false,
      "title": "No coronal view",
      "description": "No coronal view."
    },
    "no_sag": {
      "type": "boolean",
      "default": false,
      "title": "No sagittal view",
      "description": "No sagittal view."
    },
    "no_axi": {
      "type": "boolean",
      "default": false,
      "title": "No axial view",
      "description": "No axial view."
    },
    "echo": {
      "type": "boolean",
      "default": false,
      "title": "Echo",
      "description": "Echo the command line arguments."
    }
  },
  "additionalProperties": false
}