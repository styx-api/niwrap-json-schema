{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Determine if a file is oblique or plumb",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "@isOblique"
    },
    "infile": {
      "type": "string",
      "title": "Input file",
      "description": "Input file (e.g., Hello+orig.HEAD)"
    }
  },
  "additionalProperties": false,
  "required": [
    "infile"
  ]
}