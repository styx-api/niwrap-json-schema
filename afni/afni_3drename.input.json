{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "3drename",
  "description": "Tool to rename AFNI datasets by changing the dataset prefix.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/3drename"
    },
    "old_prefix": {
      "type": "string",
      "title": "Old Prefix",
      "description": "Old prefix of the datasets to rename."
    },
    "new_prefix": {
      "type": "string",
      "title": "New Prefix",
      "description": "New prefix for the datasets."
    }
  },
  "additionalProperties": false,
  "required": [
    "old_prefix",
    "new_prefix"
  ]
}