{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "afni_check_omp",
  "description": "Tool to check the OpenMP multi-threading environment for AFNI.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/afni_check_omp"
    },
    "iterations": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Number of iterations",
      "description": "Number of iterations to run"
    }
  },
  "additionalProperties": false
}