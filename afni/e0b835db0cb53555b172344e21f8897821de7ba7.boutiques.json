{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Install or update AFNI binaries.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.@update.afni.binaries"
    },
    "defaults_flag": {
      "type": "boolean",
      "default": false,
      "title": "Defaults Flag",
      "description": "Install current package into abin."
    },
    "help_flag": {
      "type": "boolean",
      "default": false,
      "title": "Help Flag",
      "description": "Show this help."
    },
    "help_sys_progs_flag": {
      "type": "boolean",
      "default": false,
      "title": "Help Sys Progs Flag",
      "description": "List system programs that block update."
    },
    "apsearch": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Aps Search",
      "description": "Specify getting apsearch updates."
    },
    "bindir": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Binary Directory",
      "description": "Set AFNI binary directory to ABIN."
    },
    "curl_flag": {
      "type": "boolean",
      "default": false,
      "title": "Curl Flag",
      "description": "Default to curl instead of wget."
    },
    "do_dotfiles_flag": {
      "type": "boolean",
      "default": false,
      "title": "Do Dotfiles Flag",
      "description": "Try to initialize dot files if needed."
    },
    "do_extras_flag": {
      "type": "boolean",
      "default": false,
      "title": "Do Extras Flag",
      "description": "Do extra niceties (beyond simple install)."
    },
    "echo_flag": {
      "type": "boolean",
      "default": false,
      "title": "Echo Flag",
      "description": "Turn on shell command echo."
    },
    "make_backup": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Make Backup",
      "description": "Make a backup of binaries before replacing."
    },
    "no_cert_verify_flag": {
      "type": "boolean",
      "default": false,
      "title": "No Cert Verify Flag",
      "description": "Do not verify the server CA certificate."
    },
    "no_recur_flag": {
      "type": "boolean",
      "default": false,
      "title": "No Recur Flag",
      "description": "Do not download and run new @uab script."
    },
    "proto": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Protocol",
      "description": "Access afni host via specified PROTOCOL."
    },
    "quick_flag": {
      "type": "boolean",
      "default": false,
      "title": "Quick Flag",
      "description": "Quick mode, no fancies."
    },
    "show_obsoletes_flag": {
      "type": "boolean",
      "default": false,
      "title": "Show Obsoletes Flag",
      "description": "List any obsolete packages."
    },
    "show_obsoletes_grep_flag": {
      "type": "boolean",
      "default": false,
      "title": "Show Obsoletes Grep Flag",
      "description": "List any obsolete packages (easy to grep)."
    },
    "show_system_progs_flag": {
      "type": "boolean",
      "default": false,
      "title": "Show System Progs Flag",
      "description": "Show system programs that do not belong in abin."
    },
    "sys_ok_flag": {
      "type": "boolean",
      "default": false,
      "title": "Sys OK Flag",
      "description": "OK to update, even if system progs found."
    },
    "test_flag": {
      "type": "boolean",
      "default": false,
      "title": "Test Flag",
      "description": "Just attempt the download and quit."
    },
    "test_protos_flag": {
      "type": "boolean",
      "default": false,
      "title": "Test Protos Flag",
      "description": "Test download protocols and exit."
    },
    "revert_flag": {
      "type": "boolean",
      "default": false,
      "title": "Revert Flag",
      "description": "Revert binaries to previous version."
    },
    "local_package": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Local Package",
      "description": "Install local PACKAGE.tgz package."
    },
    "prog_list": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Program List",
      "description": "Install given programs, not whole PACKAGE."
    },
    "package": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Package",
      "description": "Install distribution package PACKAGE."
    }
  },
  "additionalProperties": false
}