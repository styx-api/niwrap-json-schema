{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "3dTrackID",
  "description": "FACTID-based tractography code for AFNI, part of FATCAT",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "indimap": {
      "type": "string",
      "title": "INDIMAP",
      "description": "Output of INDIMAP"
    },
    "pairmap": {
      "type": "string",
      "title": "PAIRMAP",
      "description": "Output of PAIRMAP"
    },
    "grid": {
      "type": "string",
      "title": "Statistics grid file",
      "description": "Text file output of statistics of WM-ROIs"
    },
    "niml_tract": {
      "type": "string",
      "title": "Track output for SUMA",
      "description": "Track-like output for viewing in SUMA"
    },
    "niml_dset": {
      "type": "string",
      "title": "NIML dataset",
      "description": "Dataset output for use with *.niml.tract"
    },
    "trk": {
      "type": "string",
      "title": "TrackVis output",
      "description": "TrackVis-like output for viewing in TrackVis"
    },
    "pairmap_labeltable": {
      "type": "string",
      "title": "PAIRMAP labeltable",
      "description": "Output of PAIRMAP labeltable"
    },
    "roi_labels": {
      "type": "string",
      "title": "ROI labels output",
      "description": "Output file of all ROI labels"
    },
    "option_values": {
      "type": "string",
      "title": "Option values output",
      "description": "Output of all option values"
    }
  }
}