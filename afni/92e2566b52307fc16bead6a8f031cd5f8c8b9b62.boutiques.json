{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Creates a script to compute p-value and q-value curves.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.make_pq_script.py"
    },
    "dataset": {
      "type": "string",
      "title": "Input dataset",
      "description": "Input dataset (no sub-brick selectors)"
    },
    "brick_index": {
      "type": "number",
      "title": "Brick Index",
      "description": "Volume sub-brick for specific t-stat"
    },
    "mask": {
      "type": "string",
      "title": "Mask dataset",
      "description": "Mask volume dataset"
    },
    "out_script": {
      "type": "string",
      "title": "Output script",
      "description": "Name for output script to write"
    }
  },
  "additionalProperties": false,
  "required": [
    "dataset",
    "brick_index",
    "mask",
    "out_script"
  ]
}