{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "count",
  "description": "Numbered copies generator with custom format support and random sequence options",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.count"
    },
    "bot": {
      "type": "string",
      "title": "Beginning Value",
      "description": "Starting number or character"
    },
    "top": {
      "type": "string",
      "title": "Ending Value",
      "description": "Ending number or character"
    },
    "step": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Step Increment",
      "description": "Stride step or mode (integer step size, R#, S# or S)"
    },
    "seed": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Seed Number",
      "description": "Seed number for random number generator"
    },
    "sseed": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Seed String",
      "description": "Seed string for random number generator"
    },
    "column": {
      "type": "boolean",
      "default": false,
      "title": "Column Output",
      "description": "Write output, one number per line"
    },
    "digits": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Digit Count",
      "description": "Number of digits to print"
    },
    "form": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Format String",
      "description": "Custom format string for printing the numbers"
    },
    "root": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Prefix String",
      "description": "String to print before the number"
    },
    "sep": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Separator Character",
      "description": "Separator character between the numbers"
    },
    "suffix": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Suffix String",
      "description": "String to print after the number"
    },
    "scale": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Scale Factor",
      "description": "Scale factor to multiply each number"
    },
    "comma": {
      "type": "boolean",
      "default": false,
      "title": "Comma Separator",
      "description": "Put commas between the outputs"
    },
    "skipnmodm": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Skip Modulus",
      "description": "Skip numbers with modulus"
    }
  },
  "additionalProperties": false,
  "required": [
    "bot",
    "top"
  ]
}