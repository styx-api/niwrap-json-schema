{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fftest",
  "description": "A command line tool for testing purposes",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.fftest"
    },
    "length": {
      "type": "number",
      "title": "Length",
      "description": "Length of the test"
    },
    "num_tests": {
      "type": "number",
      "title": "Number of Tests",
      "description": "Number of tests to run"
    },
    "vector_size": {
      "type": "number",
      "title": "Vector Size",
      "description": "Vector size for the test"
    },
    "quiet_mode": {
      "type": "boolean",
      "default": false,
      "title": "Quiet Mode",
      "description": "Quiet mode"
    }
  },
  "additionalProperties": false,
  "required": [
    "length",
    "num_tests",
    "vector_size"
  ]
}