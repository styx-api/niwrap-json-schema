{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "to3d",
  "description": "Creates 3D datasets for use with AFNI from 2D image files",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "headfile": {
      "type": "string",
      "title": "Output HEAD File",
      "description": "Output AFNI HEAD file"
    },
    "brikfile": {
      "type": "string",
      "title": "Output BRIK File",
      "description": "Output AFNI BRIK file"
    },
    "outfile_outliers": {
      "type": "string",
      "title": "Outlier Count File",
      "description": "Outlier count file"
    }
  }
}