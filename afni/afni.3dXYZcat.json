{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Catenates datasets spatially.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.3dXYZcat"
    },
    "direction": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Direction",
      "description": "Catenate along direction 'Q' (X, Y, Z, or their synonyms I, J, K)."
    },
    "prefix": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output prefix",
      "description": "Use 'pname' for the output dataset prefix name."
    },
    "verbose": {
      "type": "boolean",
      "default": false,
      "title": "Verbose",
      "description": "Print out some verbositiness as the program proceeds."
    },
    "datasets": {
      "type": "array",
      "items": {
        "type": "string",
        "x-styx-type": "file"
      },
      "title": "Input datasets",
      "description": "Input datasets to concatenate."
    }
  },
  "additionalProperties": false,
  "required": [
    "datasets"
  ]
}