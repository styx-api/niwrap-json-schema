{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "@FSlabel2dset",
  "description": "A script to convert a FreeSurfer ASCII label file into a SUMA dataset and a SUMA ROI",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni/@FSlabel2dset"
    },
    "fs_label_file": {
      "type": "string",
      "x-styx-type": "file",
      "title": "FreeSurfer Label File",
      "description": "Specify the ASCII label file from FreeSurfer"
    },
    "val": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Node Value",
      "description": "Assign integer VAL to the nodes in FS_LABEL_FILE (Default is 1)"
    },
    "help": {
      "type": "boolean",
      "default": false,
      "title": "Help Flag",
      "description": "Display help message"
    },
    "echo": {
      "type": "boolean",
      "default": false,
      "title": "Echo Debug Flag",
      "description": "Turn echo for debugging"
    },
    "keep_tmp": {
      "type": "boolean",
      "default": false,
      "title": "Keep Temporary Files Flag",
      "description": "Don't cleanup temp files"
    }
  },
  "additionalProperties": false,
  "required": [
    "fs_label_file",
    "@type"
  ]
}