{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "3dTwotoComplex",
  "description": "Converts 2 sub-bricks of input to a complex-valued dataset",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "out_brick": {
      "type": "string",
      "title": "Output BRIK file",
      "description": "Output complex-valued dataset with prefix"
    },
    "out_head": {
      "type": "string",
      "title": "Output HEAD file",
      "description": "Header for the complex-valued dataset"
    }
  }
}