    "gltsym_label": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Symbolic GLT Labels",
      "description": "Set corresponding GLT labels"
    },
    "motion_limit": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Motion Limit",
      "description": "Set per-TR motion limit in mm"
    },
    "outlier_limit": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
