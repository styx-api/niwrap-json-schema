    "regress_mot_deriv": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Regress Motion Derivatives",
      "description": "Regress motion derivatives (yes/no)"
    },
    "regress_opts_3dD": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Regress Options 3dD",
      "description": "Specify extra options for 3dDeconvolve"
    },
    "reml_exec": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
