{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "uber_subject.py",
  "description": "Graphical interface to afni_proc.py",
  "type": "object",
  "properties": {
    "@type": {
      "const": "afni.uber_subject.py"
    },
    "qt_opts": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Qt Options",
      "description": "Pass options to PyQt4"
    },
    "svar": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Subject Variable",
      "description": "Set subject variable to value"
    },
    "cvar": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Control Variable",
      "description": "Set control variable to value"
    },
    "no_gui": {
      "type": "boolean",
      "default": false,
      "title": "No GUI Flag",
      "description": "Do not open graphical interface"
    },
    "print_ap_command": {
      "type": "boolean",
      "default": false,
      "title": "Print AP Command Flag",
      "description": "Show afni_proc.py script"
    },
    "save_ap_command": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Save AP Command",
      "description": "Save afni_proc.py script"
    },
    "exec_ap_command": {
      "type": "boolean",
      "default": false,
      "title": "Exec AP Command Flag",
      "description": "Run afni_proc.py command"
    },
    "exec_proc_script": {
      "type": "boolean",
      "default": false,
      "title": "Exec Proc Script Flag",
      "description": "Run proc script"
    },
    "align_cost": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Alignment Cost Function",
      "description": "Specify cost function for anat/EPI alignment"
    },
    "align_giant_move": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
