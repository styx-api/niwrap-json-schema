{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "tedana_wrapper.py",
  "description": "Internal wrapper to run tedana.py, typically used within afni_proc.py.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
