{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "robustfov",
  "description": "Reduce FOV of image to remove lower head and neck.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "output_roi_volume": {
      "type": "string",
      "title": "Output ROI volume",
      "description": "ROI volume output"
    },
    "output_matrix_file": {
      "type": "string",
      "title": "Output matrix file",
      "description": "Matrix output (ROI to full FOV)"
    }
  }
}