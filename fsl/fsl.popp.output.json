{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "popp",
  "description": "Physiological data processing tool of FSL",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "processed_data_output": {
      "type": "string",
      "title": "Processed Data Output",
      "description": "Processed physiological data output"
    },
    "timing_output": {
      "type": "string",
      "title": "Timing and Triggers Output",
      "description": "Timing and triggers output"
    },
    "rvt_data_output": {
      "type": "string",
      "title": "RVT Data Output",
      "description": "RVT data output file"
    },
    "heartrate_data_output": {
      "type": "string",
      "title": "Heartrate Data Output",
      "description": "Heartrate data output file"
    }
  }
}