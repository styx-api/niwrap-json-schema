{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fabber_cest",
  "description": "Fabber Model-based Analysis",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "logfile": {
      "type": "string",
      "title": "Log file",
      "description": "Log file"
    },
    "modelfit_out": {
      "type": "string",
      "title": "Model Fit File",
      "description": "Model fit output as a 4d volume"
    },
    "residuals_out": {
      "type": "string",
      "title": "Residuals File",
      "description": "Residuals output as a 4d volume"
    },
    "modelextras_out": {
      "type": "string",
      "title": "Model Extras File",
      "description": "Model extras output"
    },
    "mvn_out": {
      "type": "string",
      "title": "MVN Distributions File",
      "description": "Final MVN distributions output"
    },
    "mean_out": {
      "type": "string",
      "title": "Parameter Means File",
      "description": "Parameter means output"
    },
    "std_out": {
      "type": "string",
      "title": "Parameter Std Dev File",
      "description": "Parameter standard deviations output"
    },
    "var_out": {
      "type": "string",
      "title": "Parameter Variances File",
      "description": "Parameter variances output"
    },
    "zstat_out": {
      "type": "string",
      "title": "Parameter Z Stats File",
      "description": "Parameter Z statistics output"
    },
    "noise_mean_out": {
      "type": "string",
      "title": "Noise Means File",
      "description": "Noise means output"
    },
    "noise_std_out": {
      "type": "string",
      "title": "Noise Std Dev File",
      "description": "Noise standard deviations output"
    },
    "free_energy_out": {
      "type": "string",
      "title": "Free Energy File",
      "description": "Free energy output"
    }
  }
}