{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "betsurf",
  "description": "BET Surface Finder to extract brain surfaces using T1 and T2 images",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "output_mask": {
      "type": "string",
      "title": "Output Mask",
      "description": "Generated binary mask"
    },
    "output_outline": {
      "type": "string",
      "title": "Output Outline",
      "description": "Generated surface outline"
    },
    "output_skull": {
      "type": "string",
      "title": "Output Skull Mask",
      "description": "Generated skull mask"
    }
  }
}