{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Display information about NIFTI-1 image file",
  "type": "object",
  "properties": {
    "__STYXTYPE__": {
      "const": "fslinfo"
    },
    "filename": {
      "type": "string",
      "title": "Input file",
      "description": "Input NIFTI-1 image file (e.g. img.nii.gz)"
    }
  },
  "additionalProperties": false,
  "required": [
    "filename"
  ]
}