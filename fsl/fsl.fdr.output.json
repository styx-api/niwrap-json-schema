{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fdr",
  "description": "False Discovery Rate (FDR) correction tool from FSL",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "output_adjusted": {
      "type": "string",
      "title": "Output adjusted p-value image",
      "description": "Output FDR-adjusted p-values image"
    },
    "output_thresholded": {
      "type": "string",
      "title": "Output thresholded p-value image",
      "description": "Thresholded output p-value image"
    },
    "output_order": {
      "type": "string",
      "title": "Output order values image",
      "description": "Output order values image"
    }
  }
}