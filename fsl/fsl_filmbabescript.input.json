{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "filmbabescript",
  "description": "A tool/script for processing FEAT directories and FLOBs directories",
  "type": "object",
  "properties": {
    "@type": {
      "const": "fsl/filmbabescript"
    },
    "feat_dir": {
      "type": "string",
      "title": "FEAT Directory",
      "description": "Input FEAT directory"
    },
    "flobs_dir": {
      "type": "string",
      "title": "FLOBs Directory",
      "description": "Input FLOBs directory"
    }
  },
  "additionalProperties": false,
  "required": [
    "feat_dir",
    "flobs_dir"
  ]
}