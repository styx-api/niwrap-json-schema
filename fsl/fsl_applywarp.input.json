{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "applywarp",
  "description": "Apply warps estimated by FNIRT (or some other software) to some image.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "fsl/applywarp"
    },
    "interp": {
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "nn",
            "trilinear",
            "sinc",
            "spline"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Interp",
      "description": "'nn' or 'trilinear' or 'sinc' or 'spline'. Interpolation method."
    },
    "in_file": {
      "type": "string",
      "x-styx-type": "file",
      "title": "In file",
      "description": "Image to be warped."
    },
    "ref_file": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Ref file",
      "description": "Reference image."
    },
    "out_file": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Out file",
      "description": "Output filename."
    },
    "relwarp": {
      "type": "boolean",
      "default": false,
      "title": "Relwarp",
      "description": "Treat warp field as relative: x' = x + w(x)."
    },
    "abswarp": {
      "type": "boolean",
      "default": false,
      "title": "Abswarp",
      "description": "Treat warp field as absolute: x' = w(x)."
    },
    "datatype": {
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "char",
            "short",
            "int",
            "float",
            "double"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Datatype",
      "description": "'char' or 'short' or 'int' or 'float' or 'double'. Force output data type [char short int float double]."
    },
    "field_file": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Field file",
      "description": "File containing warp field."
    },
    "mask_file": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Mask file",
      "description": "Filename for mask image (in reference space)."
    },
    "output_type": {
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "NIFTI",
            "NIFTI_PAIR",
            "NIFTI_GZ",
            "NIFTI_PAIR_GZ"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Output type",
      "description": "'nifti' or 'nifti_pair' or 'nifti_gz' or 'nifti_pair_gz'. Fsl output type."
    },
    "postmat": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Postmat",
      "description": "Filename for post-transform (affine matrix)."
    },
    "premat": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Premat",
      "description": "Filename for pre-transform (affine matrix)."
    },
    "superlevel": {
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "a"
          ]
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Superlevel",
      "description": "'a' or an integer. Level of intermediary supersampling, a for 'automatic' or integer level. default = 2."
    },
    "superlevel_2": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Superlevel",
      "description": "'a' or an integer. Level of intermediary supersampling, a for 'automatic' or integer level. default = 2."
    },
    "supersample": {
      "type": "boolean",
      "default": false,
      "title": "Supersample",
      "description": "Intermediary supersampling of output, default is off."
    }
  },
  "additionalProperties": false,
  "required": [
    "in_file",
    "ref_file"
  ]
}