{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fabber_asl",
  "description": "Fabber is a tool for automated model fitting of time series data.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "logfile": {
      "type": "string",
      "title": "Log file",
      "description": "Log file of Fabber output"
    },
    "modelfit": {
      "type": "string",
      "title": "Model fit output",
      "description": "Model fit output"
    },
    "residuals": {
      "type": "string",
      "title": "Residuals output",
      "description": "Residuals output"
    },
    "model_extras": {
      "type": "string",
      "title": "Model extras output",
      "description": "Model specific timeseries data"
    },
    "mvn": {
      "type": "string",
      "title": "MVN distributions output",
      "description": "MVN distributions"
    },
    "means": {
      "type": "string",
      "title": "Parameter means output",
      "description": "Parameter means"
    },
    "stds": {
      "type": "string",
      "title": "Parameter standard deviations output",
      "description": "Parameter standard deviations"
    },
    "variances": {
      "type": "string",
      "title": "Parameter variances output",
      "description": "Parameter variances"
    },
    "zstats": {
      "type": "string",
      "title": "Parameter Zstats output",
      "description": "Parameter Zstats"
    },
    "noise_means": {
      "type": "string",
      "title": "Noise means output",
      "description": "Noise means"
    },
    "noise_stds": {
      "type": "string",
      "title": "Noise standard deviations output",
      "description": "Noise standard deviations"
    },
    "free_energy": {
      "type": "string",
      "title": "Free energy output",
      "description": "Free energy"
    }
  }
}