{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "filmbabe",
  "description": "FILM with MCMC-based Bayesian Analysis for fMRI",
  "type": "object",
  "properties": {
    "@type": {
      "const": "fsl/filmbabe"
    },
    "datafile": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Data file",
      "description": "Data file"
    },
    "datafile_alias": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Data file alias",
      "description": "Data file"
    },
    "mask": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Mask file",
      "description": "Mask file"
    },
    "mask_alias": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Mask file alias",
      "description": "Mask file"
    },
    "designfile": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Design matrix file",
      "description": "Design matrix file"
    },
    "designfile_alias_1": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Design matrix file alias 1",
      "description": "Design matrix file"
    },
    "designfile_alias_2": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Design matrix file alias 2",
      "description": "Design matrix file"
    },
    "frf": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Regressors file",
      "description": "File indicating which regressors belong to which original EV design matrix file (a -1 label indicates a non-flobs regressor)"
    },
    "verbose_flag": {
      "type": "boolean",
      "default": false,
      "title": "Verbose Flag",
      "description": "Switch on diagnostic messages"
    },
    "verbose_flag_alias": {
      "type": "boolean",
      "default": false,
      "title": "Verbose Flag alias",
      "description": "Switch on diagnostic messages"
    },
    "debug_level": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Debug level",
      "description": "Set debug level"
    },
    "debug_level_alias_1": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Debug level alias1",
      "description": "Set debug level"
    },
    "debug_level_alias_2": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Debug level alias2",
      "description": "Set debug level"
    },
    "timing_on_flag": {
      "type": "boolean",
      "default": false,
      "title": "Timing flag",
      "description": "Turn timing on"
    },
    "help_flag": {
      "type": "boolean",
      "default": false,
      "title": "Help flag",
      "description": "Display help message"
    },
    "help_flag_alias": {
      "type": "boolean",
      "default": false,
      "title": "Help flag alias",
      "description": "Display help message"
    },
    "flobs_prior_off_flag": {
      "type": "boolean",
      "default": false,
      "title": "Flobs prior off flag",
      "description": "Turn FLOBS prior off"
    },
    "flobs_prior_off_alias": {
      "type": "boolean",
      "default": false,
      "title": "Flobs prior off flag alias",
      "description": "Turn FLOBS prior off"
    },
    "flobs_dir": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "FLOBS directory",
      "description": "FLOBS directory; required when using FLOBS constraints"
    },
    "prior_covar_file": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Prior covariance file",
      "description": "Prior covariance matrix file"
    },
    "prior_covar_file_alias": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Prior covariance file alias",
      "description": "Prior covariance matrix file"
    },
    "prior_mean_file": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Prior mean file",
      "description": "Prior mean matrix file"
    },
    "prior_mean_file_alias": {
      "anyOf": [
        {
          "type": "string",
          "x-styx-type": "file"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Prior mean file alias",
      "description": "Prior mean matrix file"
    },
    "log_dir": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Log directory",
      "description": "Log directory"
    },
    "log_dir_alias_1": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Log directory alias1",
      "description": "Log directory"
    },
    "log_dir_alias_2": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Log directory alias2",
      "description": "Log directory"
    },
    "num_iterations": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Number of iterations",
      "description": "Number of VB iterations; default is 5"
    },
    "temporal_ar_mrf_prec": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Temporal AR MRF precision",
      "description": "MRF precision to impose on temporal AR maps, default is -1 for a proper full Bayes approach"
    },
    "temporal_ar_mrf_prec_alias": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Temporal AR MRF precision alias",
      "description": "MRF precision to impose on temporal AR maps, default is -1 for a proper full Bayes approach"
    },
    "temporal_ar_flag": {
      "type": "boolean",
      "default": false,
      "title": "Temporal AR flag",
      "description": "Impose ARD/MRF on temporal AR"
    },
    "num_trace_samples": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Number of trace samples",
      "description": "Number of samples to take to estimate trace; default is 0 (uses only diagonal elements of precision matrix to estimate trace)"
    },
    "num_trace_samples_alias": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Number of trace samples alias",
      "description": "Number of samples to take to estimate trace; default is 0 (uses only diagonal elements of precision matrix to estimate trace)"
    },
    "temporal_ar_order": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Temporal AR order",
      "description": "Order of temporal AR; default is 3"
    }
  },
  "additionalProperties": false,
  "required": [
    "datafile",
    "datafile_alias",
    "mask",
    "mask_alias",
    "designfile",
    "designfile_alias_1",
    "designfile_alias_2",
    "frf"
  ]
}