{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fslsmoothfill",
  "description": "Smoothfill is a tool designed to fill in holes in images by smoothly interpolating the pixel values.",
  "type": "object",
  "properties": {
    "@type": {
      "const": "fsl/fslsmoothfill"
    },
    "input_image": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Input Image",
      "description": "Filename of the input image."
    },
    "mask_image": {
      "type": "string",
      "x-styx-type": "file",
      "title": "Mask Image",
      "description": "Filename of the mask image."
    },
    "output_image": {
      "type": "string",
      "title": "Output Image",
      "description": "Filename for the output smoothed result image."
    },
    "number_of_iterations": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Number of Iterations",
      "description": "Number of iterations."
    },
    "debug_flag": {
      "type": "boolean",
      "default": false,
      "title": "Debug Flag",
      "description": "Turn on debugging output."
    },
    "verbose_flag": {
      "type": "boolean",
      "default": false,
      "title": "Verbose Flag",
      "description": "Switch on diagnostic messages."
    }
  },
  "additionalProperties": false,
  "required": [
    "input_image",
    "mask_image",
    "output_image",
    "@type"
  ]
}