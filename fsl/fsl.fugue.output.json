{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fugue",
  "description": "FMRIB's Utility for Geometric Unwarping of EPIs",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "fmap_out_file_outfile": {
      "type": "string",
      "title": "Fmap out file",
      "description": "Fieldmap file."
    },
    "shift_out_file_outfile": {
      "type": "string",
      "title": "Shift out file",
      "description": "Voxel shift map file."
    },
    "unwarped_file_outfile": {
      "type": "string",
      "title": "Unwarped file",
      "description": "Unwarped file."
    },
    "warped_file_outfile": {
      "type": "string",
      "title": "Warped file",
      "description": "Forward warped file."
    }
  }
}