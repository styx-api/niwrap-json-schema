{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fnirt",
  "description": "FSL non-linear registration",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "field_file_outfile": {
      "type": "string",
      "title": "Field file",
      "description": "File with warp field."
    },
    "fieldcoeff_file_outfile": {
      "type": "string",
      "title": "Fieldcoeff file",
      "description": "File with field coefficients."
    },
    "jacobian_file_outfile": {
      "type": "string",
      "title": "Jacobian file",
      "description": "File containing jacobian of the field."
    },
    "log_file_outfile": {
      "type": "string",
      "title": "Log file",
      "description": "Name of log-file."
    },
    "modulatedref_file_outfile": {
      "type": "string",
      "title": "Modulatedref file",
      "description": "File containing intensity modulated --ref."
    },
    "warped_file_outfile": {
      "type": "string",
      "title": "Warped file",
      "description": "Warped image."
    }
  }
}