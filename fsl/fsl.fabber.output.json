{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "fabber",
  "description": "Fabber is a tool for model-based Bayesian analysis of time-series data",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "paramnames_file": {
      "type": "string",
      "title": "Parameter Names File",
      "description": "File containing the names of the model parameters"
    },
    "model_fit_file": {
      "type": "string",
      "title": "Model Fit File",
      "description": "The model fit output file"
    },
    "residuals_file": {
      "type": "string",
      "title": "Residuals File",
      "description": "The residuals output file"
    },
    "model_extras_file": {
      "type": "string",
      "title": "Model Extras File",
      "description": "The model extras output file"
    },
    "mvn_file": {
      "type": "string",
      "title": "MVN File",
      "description": "The MVN distributions output file"
    },
    "mean_file": {
      "type": "string",
      "title": "Mean File",
      "description": "The parameter means output file"
    },
    "std_file": {
      "type": "string",
      "title": "Standard Deviations File",
      "description": "The parameter standard deviations output file"
    },
    "var_file": {
      "type": "string",
      "title": "Variances File",
      "description": "The parameter variances output file"
    },
    "zstat_file": {
      "type": "string",
      "title": "Z-Stats File",
      "description": "The parameter Zstats output file"
    },
    "noise_mean_file": {
      "type": "string",
      "title": "Noise Means File",
      "description": "The noise means output file"
    },
    "noise_std_file": {
      "type": "string",
      "title": "Noise Standard Deviations File",
      "description": "The noise standard deviations output file"
    },
    "free_energy_file": {
      "type": "string",
      "title": "Free Energy File",
      "description": "The free energy output file"
    }
  }
}