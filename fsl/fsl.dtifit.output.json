{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "dtifit",
  "description": "DTIFIT - Fit a diffusion tensor model at each voxel",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "root": {
      "type": "string",
      "title": "Root",
      "description": "Output root directory"
    },
    "fa_output": {
      "type": "string",
      "title": "FA output",
      "description": "Fractional anisotropy output"
    },
    "md_output": {
      "type": "string",
      "title": "MD output",
      "description": "Mean diffusivity output"
    },
    "mo_output": {
      "type": "string",
      "title": "MO output",
      "description": "Mode of the anisotropy output"
    },
    "l1_output": {
      "type": "string",
      "title": "L1 output",
      "description": "1st eigenvalue output"
    },
    "l2_output": {
      "type": "string",
      "title": "L2 output",
      "description": "2nd eigenvalue output"
    },
    "l3_output": {
      "type": "string",
      "title": "L3 output",
      "description": "3rd eigenvalue output"
    }
  }
}